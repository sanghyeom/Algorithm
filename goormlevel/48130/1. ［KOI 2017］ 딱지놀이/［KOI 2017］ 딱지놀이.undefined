N = int(input())
for _ in range(N):
	A_cnt = [0]*5
	B_cnt = [0]*5
	
	A = list(map(int,input().split()))
	B = list(map(int,input().split()))
	result = 'D'
	
	for i in range(1,A[0]+1):
		A_cnt[A[i]] += 1
	for j in range(1,B[0]+1):
		B_cnt[B[j]] += 1

	for h in range(4,0,-1):
		if A_cnt[h]>B_cnt[h] :
			result = 'A'
		elif A_cnt[h] < B_cnt[h]:
			result = 'B'
		elif A_cnt[h] == B_cnt[h]:
			result = 'D'

		if result != 'D' :
			break
 
	print(result)